    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script>

        $(document).ready(function () {

            $.ajax({
                type: "GET",
                url: "/Studio/getLogoName",
                data: "{}",
                success: function (data) {
                    $("#lnkLogo1").attr("href", "../../StudioLogo/" + data[0].name);
                    $("#lnkLogo2").attr("href", "../../StudioLogo/" + data[0].name);
                    $("#imgadminLogo").attr("src", "../../StudioLogo/" + data[0].name);
                }
            });
        });
    </script>
    <script>
        function fnLogout() {

            $.ajax({
                async: true,
                type: 'POST',
                dataType: 'JSON',
                contentType: 'application/json; charset=utf-8',
                data: "",
                url: '/Login/LogOut',
                success: function (response) {
                    if (response.success === true) {
                        toastr.success(response.message, 'You are logging out!', {
                            timeOut: 1000, fadeOut: 1000,
                            onHidden: function () { window.location.href = "/Login/Login"; }
                        });
                    }
                },
                error: function () {
                    toastr.error('Error occur', 'Error occur while loging out!.', { timeOut: 1000 });
                    return;
                }

            });
        }


    </script>
