    <script src="~/userassets/vendor/jquery.easing.min.js"></script>
    <script src="~/userassets/vendor/isotope.pkgd.min.js"></script>
    <script src="~/userassets/vendor/imagesloaded.pkgd.min.js"></script>
    <script src="~/userassets/vendor/jquery.nicescroll.min.js"></script>
    <script src="~/userassets/vendor/owl-carousel/js/owl.carousel.min.js"></script>
    <script src="~/userassets/vendor/jquery.mousewheel.min.js"></script>
    <script src="~/userassets/vendor/ytplayer/js/jquery.mb.YTPlayer.min.js"></script>
    <script src="~/userassets/vendor/lightgallery/js/lightgallery.min.js"></script>
    <script src="~/userassets/vendor/lightgallery/js/lightgallery-plugins.js"></script>
    <script src="~/userassets/js/theme.js"></script>


