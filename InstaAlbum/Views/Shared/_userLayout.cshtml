<body class="temp-uppercase temp-grayscale animsition">


    <span class="page-cover"></span>


    <div id="header" class="header-hover">

        <div class="header-close" title="Close">×</div>
        <div class="logo-small">
            <a href="index.html"><img src="~/userassets/img/logo3.png" title="Home" alt="logo"></a>
        </div>
        <span class="header-menu-icon" title="Open Menu"><i class="fa fa-bars"></i></span>
        <a href="#" class="header-contact-icon" title="Contact Me"><i class="fa fa-envelope"></i></a>
        <div class="header-inner">
            <div class="header-top">
                <div class="logo-big">
                    <a href="index.html"><img src="~/userassets/img/logo3.png" title="Home" alt="logo"></a>
                </div>
            </div>
            <div class="header-middle">
                <div id="menu">
                    <ul class="menu-list">
                        <li class="has-children active">
                            <a href="@Url.Action(" Index","UserHome")" class="sub-menu-trigger">Home</a>
                        </li>
                        <li class="has-children">
                            <a href="@Url.Action(" About","UserHome")" class="sub-menu-trigger">About Us</a>
                        </li>
                        <li class="has-children">
                            <a href="#" class="sub-menu-trigger">Gallery</a>
                        </li>
                        <li class="has-children">
                            <a href="#" class="sub-menu-trigger">Protfolio</a>
                        </li>
                        <li class="has-children">
                            <a href="@Url.Action(" Contact","UserHome")" class="sub-menu-trigger">Contact</a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="header-bottom">
                <div class="follow-me-buttons">
                    <ul>
                        <li><a href="#" title="Follow me on Facebook" target="_blank"><i class="fa fa-facebook"></i></a></li>
                        <li><a href="#" title="Follow me on Twitter" target="_blank"><i class="fa fa-twitter"></i></a></li>
                        <li><a href="#" title="Follow me on Instagram" target="_blank"><i class="fa fa-instagram"></i></a></li>
                        <li><a href="mailto:info@your-email.com" title="Email Me" target="_blank"><i class="fa fa-envelope"></i></a></li>
                    </ul>
                </div>
                <div class="copyright">
                    Copyright © 2016 <br>
                    <a target="_blank" href="https://themeforest.net/user/themetorium/portfolio?ref=Themetorium">Themetorium</a>
                </div>
            </div>
        </div>
    </div>

    <div id="mobile-header">
        <div class="logo-mobile">
            <a href="index.html"><img src="~/userassets/img/logo-small.png" alt="logo"></a>
        </div>
        <div class="mobile-menu-trigger"><span></span></div>
    </div>

    <div id="body-content">
        @RenderBody()
    </div>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.0.2/js/toastr.min.js"></script>
    <script src="~/userassets/js/jquery-3.5.1.min.js"></script>
    <script src="~/userassets/vendor/jquery/jquery.min.js"></script> <!-- jquery JS (https://jquery.com) -->
    <script src="~/userassets/vendor/bootstrap/js/bootstrap.min.js"></script> <!-- bootstrap JS (http://getbootstrap.com) -->
